{
  "expo": {
    "name": "renting_app",
    "scheme": "renting_app",
    "slug": "renting_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./apps/native/assets/appIcons/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./apps/native/assets/appIcons/splash1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "infoPlist": {
        "NSAppTransportSecurity": {
          "allowsArbitraryLoads": true
        }
      },
      "supportsTablet": true,
      "userInterfaceStyle": "light",
      "config": {
        "usesNonExemptEncryption": false,
        "usesCleartextTraffic": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./apps/native/assets/appIcons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "userInterfaceStyle": "light",
      "permissions": ["INTERNET"],
      "package": "com.onlinerent.test",
      "config": {
        "networkSecurityConfig": "./apps/native/network_security_config.xml"
      }
    },

    "extra": {
      "eas": {
        "projectId": "8550e956-fda9-4373-bea2-4b847e0d02bb"
      }
    },

    "web": {
      "favicon": "./apps/native/assets/appIcons/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "./apps/native/plugins/withDisableForcedDarkModeAndroid.js",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ]
  }
}
