{
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\" --ignore-path .gitignore",
    "postinstall": "yarn run build"
  },
  "devDependencies": {
    "prettier": "^3.1.1",
    "turbo": "^1.13.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.21",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.23.1",
    "react-redux": "^9.1.0",
    "redux-persist": "^6.0.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "concurrently": "^8.2.2"
  },
  "resolutions": {
    "wrap-ansi": "7.0.0",
    "string-width": "4.1.0"
  }
}
